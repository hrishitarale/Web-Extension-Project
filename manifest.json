{
    "name": "My Ext",
    "description": "My Ext",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "./web-Ui/assets/images/16x16.png",
        "32": "./web-Ui/assets/images/32x32.png",
        "48": "./web-Ui/assets/images/48x48.png",
        "128": "./web-Ui/assets/images/128x128.png"
    },
    "options_page": "./web-Ui/admin-panel/admin-login/login.html",
    "action": {
        "default_popup": "./web-Ui/user-panel/login/login.html"
    },
    "permissions": [
        "tabs",
        "storage",
        "activeTab",
        "contextMenus",
        "http://localhost/*",
        "https://localhost/*"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}